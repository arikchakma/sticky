{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "window-cap",
  "description": "Enable window resizing APIs",
  "windows": ["*"],
  "permissions": [
    "core:window:default",
    "core:window:allow-set-size",
    "core:window:allow-inner-size",
    "core:window:allow-start-dragging"
  ]
}
